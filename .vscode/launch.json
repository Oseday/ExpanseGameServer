    "configurations": [
        {
            "name": ".NET Core SSH Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickRemoteProcess}",
            "pipeTransport": {
                "pipeProgram": "ssh",
                "pipeArgs": [ "centos@18.156.183.154", "-i", "E:/Stuff/Important/LSKeyeu1.pem" ],
                "debuggerPath": "~/vsdbg/vsdbg",
                "pipeCwd": "${workspaceRoot}",
                "quoteArgs": true
            },
            "sourceFileMap": {
                "/home/centos/UnityGameServer": "${workspaceRoot}"
            }
        }
    ]
}